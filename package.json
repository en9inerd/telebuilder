{
  "name": "telebuilder",
  "version": "0.1.4",
  "description": "Simple telegram bot framework based on gramjs",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "prebuild": "tsc --build --clean && rm -rf dist",
    "build": "tsc --build",
    "postbuild": "cp -r package.json package-lock.json dist",
    "test": "jest",
    "postversion": "cp -r package.json .. && cp -r package-lock.json ..",
    "prepublishOnly": "npm pkg delete scripts",
    "remscripts": "cd dist && npm pkg delete scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/en9inerd/telebuilder.git"
  },
  "keywords": [
    "telegram-api",
    "telegram",
    "bot"
  ],
  "author": "Vladimir Loskutov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/en9inerd/telebuilder/issues"
  },
  "homepage": "https://github.com/en9inerd/telebuilder#readme",
  "dependencies": {
    "big-integer": "^1.6.51",
    "config": "^3.3.9",
    "glob": "^8.1.0",
    "json5": "^2.2.3",
    "mongoose": "^6.9.0",
    "node-localstorage": "^2.2.1",
    "store2": "^2.14.2",
    "telegram": "^2.15.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@types/config": "^3.3.0",
    "@types/glob": "^8.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@types/node-localstorage": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "semantic-release": "^20.1.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
